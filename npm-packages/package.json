{
  "name": "algorithm-tool",
  "version": "1.0.4",
  "description": "Algorithm tools in Javascripts",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --timeout 5000 --compilers js:babel-register test/*.test.js",
    "test:coverage": "nyc npm run test && nyc report --reporter=text-lcov > coverage.lcov",
    "coverage": "codeclimate-test-reporter < coverage.lcov",
    "clean": "rm -rf lib",
    "build": "babel src -d lib",
    "build:production": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lulalachen/algorithm.git"
  },
  "author": "lulalachen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lulalachen/algorithm/issues"
  },
  "homepage": "https://github.com/lulalachen/algorithm#readme",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "codeclimate-test-reporter": "^0.3.1",
    "expect": "^1.20.1",
    "mocha": "^2.5.3",
    "nyc": "^6.4.4"
  },
  "nyc": {
    "exclude": [
      "test/*.test.js",
      "lib"
    ]
  }
}
